<tr class="<%= lso_risk_state_bg(details) %> status-row-<%= details.process.status %>">
  <td>
    <%= partition_id %>
  </td>

  <td>
    <%== lag_with_label details.lag %>
  </td>

  <td>
    <%== lag_with_label details.lag_stored %>
  </td>

  <td>
    <span class="badge <%= lag_trend_badge(details.lag_d) %>">
      <%= details.lag_d %>
    </span>
  </td>
  <td>
    <span class="badge <%= lag_trend_badge(details.lag_stored_d) %>">
      <%= details.lag_stored_d %>
    </span>
  </td>
  <td>
    <%== poll_state_with_change_time_label(details.poll_state, details.poll_state_ch) %>
  </td>
</tr>
