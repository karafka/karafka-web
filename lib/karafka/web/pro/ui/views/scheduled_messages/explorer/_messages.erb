<div class="data-table-wrapper">
  <table class="data-table">
    <thead>
      <tr>
        <th>Partition</th>
        <th>Offset</th>
        <th>Type</th>
        <th>Schema</th>
        <th>Created At</th>
        <th>Dispatch At</th>
        <th>Key</th>
        <th>Target Topic</th>
        <th>Target Key</th>
        <th class="options">Options</th>
      </tr>
    </thead>
    <tbody>
      <%==
        each_partial(
          @messages,
          'scheduled_messages/explorer/message'
        )
      %>
    </tbody>
  </table>

  <%== content_for :table_metadata %>
</div>
